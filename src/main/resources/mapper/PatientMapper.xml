<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ros.mapper.PatientMapper">

    <resultMap id="BaseResultMap" type="com.ros.domain.Patient">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="patientName" column="patient_name" jdbcType="VARCHAR"/>
            <result property="patientCondition" column="patient_condition" jdbcType="VARCHAR"/>
            <result property="patientAddress" column="patient_address" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,patient_name,patient_condition,
        patient_address
    </sql>
</mapper>
